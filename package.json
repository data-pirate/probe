{
  "name": "probejs-core",
  "version": "1.0.0",
  "description": "A powerful tool for traversing and investigating nested objects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "object",
    "traverse",
    "search",
    "query",
    "nested",
    "probe",
    "deep",
    "investigation",
    "JSON",
    "data",
    "structure",
    "exploration",
    "explorer",
    "exploring"
  ],
  "author": "Damanpreet Singh",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/data-pirate/probejs-core.git"
  },
  "bugs": {
    "url": "https://github.com/data-pirate/probejs-core/issues"
  },
  "homepage": "https://github.com/data-pirate/probejs-core#readme",
  "engines": {
    "node": ">=14.0.0"
  }
}
